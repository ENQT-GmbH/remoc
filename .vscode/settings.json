{
    "files.exclude": {
        "target": true,
    },
    "cSpell.languageSettings": [
        {
            "languageId": "rust",
            "includeRegExpList": [
                "CStyleComment",
                "string",
                "^//!"
            ],
        }
    ],
    "cSpell.words": [
        "bidir",
        "Bincode",
        "BTreeMap",
        "CBOR",
        "chmux",
        "clippy",
        "clonable",
        "codecov",
        "deps",
        "deserializable",
        "deserialization",
        "deserializers",
        "doctest",
        "ENQT",
        "forwardable",
        "MPSC",
        "MSRV",
        "oneshot",
        "RAII",
        "remoc",
        "remotable",
        "robj",
        "rustup",
        "Serde",
        "serializers",
        "struct",
        "structs",
        "unbuffered",
        "untrusted"
    ],
    "rust-analyzer.cargo.features": [
        "full",
        "default-codec-json",
        "codec-bincode",
        "codec-ciborium",
        "codec-json",
        "codec-message-pack",
        "codec-postcard",
        "js",
    ],
    "rust-analyzer.cargo.extraEnv": {
        "RUSTUP_TOOLCHAIN": "nightly"
    },
    "rust-analyzer.cargo.target": "wasm32-unknown-unknown"
    //"rust-analyzer.cargo.target": "wasm32-wasip1-threads",
}