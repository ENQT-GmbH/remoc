{
    "files.exclude": {
        "target": true,
        "examples/rtc/target": true,
    },
    "cSpell.languageSettings": [
        {
            "languageId": "rust",
            "includeRegExpList": [
                "CStyleComment",
                "string",
                "^//!"
            ],
        }
    ],
    "cSpell.words": [
        "bidir",
        "Bincode",
        "BTreeMap",
        "CBOR",
        "chmux",
        "clippy",
        "clonable",
        "codecov",
        "deps",
        "deserializable",
        "deserialization",
        "deserializers",
        "doctest",
        "ENQT",
        "forwardable",
        "MPSC",
        "MSRV",
        "oneshot",
        "RAII",
        "remoc",
        "remotable",
        "robj",
        "rustup",
        "Serde",
        "serializers",
        "struct",
        "structs",
        "unbuffered",
        "untrusted"
    ],
    "rust-analyzer.cargo.extraEnv": {
        "RUSTUP_TOOLCHAIN": "nightly"
    },
    "rust-analyzer.linkedProjects": [
        "Cargo.toml",
        "examples/rtc/Cargo.toml"
    ]
    // "rust-analyzer.cargo.features": [
    //     "full",
